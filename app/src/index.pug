- var imports = require('./imports');

doctype html
html
  head
    meta(charset='utf-8')
    meta(name='viewport' content='width=device-width, initial-scale=1')
  body
    div.app#app
    script(type='text/javascript' src=require('./appInit').default)
    script(type='text/javascript' src=require('jquery/dist/jquery.min').default)

    if htmlWebpackPlugin.options.mode === 'development'
      script(type='text/javascript' src=imports.dll)